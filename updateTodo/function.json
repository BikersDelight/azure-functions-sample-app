{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["put"],
      "route": "todos/{id}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "inputDocuments",
      "databaseName": "todossampleapp",
      "collectionName": "todoCollection",
      "connectionStringSetting": "cosmos_connection_string",
      "partitionKey": "/id",
      "sqlQuery": "SELECT TOP 1 * FROM s WHERE s.id = {id}"
    },
    {
      "type": "cosmosDB",
      "name": "outputDocument",
      "databaseName": "todossampleapp",
      "collectionName": "todoCollection",
      "createIfNotExists": true,
      "connectionStringSetting": "cosmos_connection_string",
      "direction": "out",
      "partitionKey": "/id"
    }
  ],
  "scriptFile": "../dist/updateTodo/index.js"
}
