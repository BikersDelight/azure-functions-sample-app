{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "todos",
      "methods": ["get"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "inputDocuments",
      "databaseName": "todossampleapp",
      "collectionName": "todoCollection",
      "connectionStringSetting": "cosmos_connection_string",
      "partitionKey": "/id",
      "sqlQuery": "SELECT * FROM t"
    }
  ],
  "scriptFile": "../dist/getTodos/index.js"
}
