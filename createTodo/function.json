{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["post"],
      "route": "todos"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "name": "outputDocument",
      "databaseName": "todossampleapp",
      "collectionName": "todoCollection",
      "createIfNotExists": true,
      "connectionStringSetting": "cosmos_connection_string",
      "direction": "out",
      "partitionKey": "/id"
    }
  ],
  "scriptFile": "../dist/createTodo/index.js"
}
